{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@src/*": [
        "./src/*"
      ],
      "@utils": [
        "./src/utils"
      ],
      "@api": [
        "./src/api"
      ],
      "@constants": [
        "./src/constants"
      ],
      "@store": [
        "./src/store"
      ],
      "@types": [
        "./src/types"
      ],
      "@theme": [
        "./src/theme"
      ],
    }
  }
}
